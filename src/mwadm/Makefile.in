srcdir = @srcdir@
VPATH = @srcdir@
prefix = @prefix@
exec_prefix = @exec_prefix@

CC = @CC@
INSTALL = @INSTALL@

INCLUDES = -I../include 

INCPATH = ../include

CFLAGS =  @CFLAGS@ $(INCLUDES)

LIBS =  -L../lib @readlinelib@ -lMidWay
LIB = @LIBS@
LDFLAGS = $(LIBS) 

OBJ = mwadm.o commands.o

EXE = mwadm

all: $(EXE)

mwadm: $(OBJ)
	$(CC) -o $@ $(OBJ) $(LIBS)

depend:
	makedepend -- $(CFLAGS) -- *.c

clean:
	rm -f mwadm $(OBJ) core 

distclean:
	rm -f Makefile *~

install: all
	$(INSTALL)  mwadm @bindir@/mwadm

uninstall:
	rm -f @bindir@/mwadm

TAGS:
	etags -a -o ../TAGS *.c *.h

# DO NOT DELETE
