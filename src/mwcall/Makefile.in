srcdir = @srcdir@
VPATH = @srcdir@
prefix = @prefix@
exec_prefix = @exec_prefix@

CC = @CC@
INSTALL = @INSTALL@

INCLUDES = -I../include 

INCPATH = ../include

CFLAGS =  @CFLAGS@ $(INCLUDES)

LIBS =  -L../lib -lMidWay

LDFLAGS = $(LIBS) 

OBJ = mwcall.o

EXE = mwcall

all: $(EXE)

$(EXE): $(OBJ)
	$(CC) -o $@ $(OBJ) $(LIBS)

clean:
	rm -f mwcall $(OBJ) core 

distclean:
	rm -f Makefile *~

install:
	$(INSTALL)  mwcall @bindir@/mwcall

uninstall:
	rm -f @bindir@/mwcall

TAGS:
	etags -a -o ../TAGS *.c *.h

depend:
	makedepend -- $(CFLAGS) -- *.c

