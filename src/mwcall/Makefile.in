
CC = @CC@
INSTALL = @INSTALL@

INCLUDES = -I../include 

INCPATH = ../include

CFLAGS =  $(INCLUDES) -g

LIBS =  -L../lib -lMidWay

LDFLAGS = $(LIBS) 

OBJ = mwcall.o

EXE = mwcall

all: $(EXE)

$(EXE): $(OBJ)
	$(CC) -o $@ $(OBJ) $(LIBS)

depend:
	makedepend -Y *.c

clean:
	rm -f mwcall $(OBJ) core 

distclean:
	rm -f Makefile *~

install:
	$(INSTALL)  mwcall @bindir@/mwcall

uninstall:
	rm -f @bindir@/mwcall

tags:
	etags -a -o ../TAGS *.c *.h

# DO NOT DELETE

mwcall.o: $(INCPATH)/MidWay.h mwcall.c 

