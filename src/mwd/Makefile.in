srcdir = @srcdir@
VPATH = @srcdir@
prefix = @prefix@
exec_prefix = @exec_prefix@

MDHOME = @top_srcdir@

CC = @CC@
INSTALL = @INSTALL@

LIBS = -lMidWay -lm

CFLAGS = @CFLAGS@ -I../include -I../liburl -DINTERVAL=1200

OBJS = mwd.o shmallocfmt.o requestparse.o tables.o watchdog.o

EXE = mwd

all: $(EXE) 

clean:
	rm -f $(EXE) $(OBJS) core

distclean:
	rm -f Makefile *~

install:
	$(INSTALL) mwd @bindir@/mwd

uninstall:
	rm -f @bindir@/mwd

mwd: ../lib/libMidWay.a $(OBJS)
	$(CC) -o $@ -g $(OBJS) -L../lib $(LIBS) 

tags:
	etags -a -o ../TAGS *.c *.h

depend:
	makedepend -- $(CFLAGS) -- *.c

# DO NOT DELETE
